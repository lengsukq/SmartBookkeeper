# 数据库配置
DATABASE_URL=sqlite+aiosqlite:///./smartbookkeeper.db

# JWT配置
SECRET_KEY=your-secret-key-change-in-production
JWT_SECRET_KEY=your-secret-key-change-in-production
ACCESS_TOKEN_EXPIRE_MINUTES=30
JWT_ALGORITHM=HS256

# 企业微信配置
WECOM_CORP_ID=your-corp-id
WECOM_AGENT_ID=your-agent-id
WECOM_SECRET=your-wecom-secret
WECOM_TOKEN=your-wecom-token
WECOM_AES_KEY=your-wecom-aes-key

# AI模型配置
AI_API_KEY=your-ai-api-key
AI_MODEL_NAME=your-ai-model-name
AI_API_BASE_URL=https://your-ai-api-base-url

# 钱迹配置
QIANJI_ENABLED=false
QIANJI_CATE_CHOOSE=true

# 穿透URL配置
PENETRATE_URL=http://localhost:8000

# 应用配置
APP_NAME=SmartBookkeeper
DEBUG=true